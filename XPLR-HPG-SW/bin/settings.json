{
    "files.encoding": "utf8",
    "astyle.executable": "${workspaceRoot}/bin/AStyle.exe",
    "astyle.c.enable": true,
    "C_Cpp.formatting": "Disabled",
    "astyle.astylerc": "${workspaceRoot}/bin/.astylerc",
    "prettier.printWidth": 120,
    "editor.wordWrap": "off",
    "editor.formatOnPaste": true,
    //"editor.formatOnSave": true,
    "editor.renderWhitespace": "all",
    //"editor.insertSpaces": true,
    //"editor.tabSize": 4,
    //"editor.detectIndentation": true,
    "makefile.extensionOutputFolder": "./.vscode",
    "idf.adapterTargetName": "esp32",
    /* Files that are not shown in vscode workspace */
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.obj": true
    },
    /* Files that will not be parsed by C/CPP extension */
    "C_Cpp.files.exclude": {
        "**/.vscode": true,
        "**/_build": true
    },
    "idf.portWin": "COM16",
    "idf.flashType": "UART",
    "idf.monitorBaudRate": "115200",
    "files.associations": {
        "esp_mac.h": "c",
        "*.inc": "c",
        "esp_netif.h": "c",
        "esp_netif_ip_addr.h": "c",
        "freertos.h": "c",
        "inet.h": "c",
        "sockets.h": "c",
        "xplr_wifi_starter.h": "c",
        "xplr_wifi_dns.h": "c",
        "xplr_wifi_webserver.h": "c",
        "string.h": "c",
        "reent.h": "c",
        "xplr_hpglib_cfg.h": "c",
        "esp_vfs.h": "c",
        "esp_spiffs.h": "c",
        "dirent.h": "c",
        "system_error": "c",
        "*.ipp": "c",
        "sdkconfig": "c",
        "esp_http_server.h": "c",
        "unistd.h": "c",
        "xplr_gnss_types.h": "c",
        "nvs.h": "c",
        "nvs_flash.h": "c",
        "queue.h": "c",
        "timer.h": "c",
        "xplr_nvs.h": "c",
        "sstream": "c",
        "regex": "c",
        "*.tcc": "c",
        "http_server.h": "c",
        "cstdio": "c",
        "fstream": "c",
        "board.h": "c",
        "esp_spi_flash.h": "c",
        "xplr_gnss.h": "c",
        "u_cfg_app_platform_specific.h": "c",
        "xplr_common.h": "c",
        "esp_system.h": "c",
        "esp_log.h": "c",
        "ets_sys.h": "c"
    },
    "cmake.configureOnOpen": false
}